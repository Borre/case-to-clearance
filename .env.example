# Huawei Cloud ModelArts MaaS Configuration
MAAS_API_KEY=your_maas_api_key_here
MAAS_REGION=cn-north-4
MAAS_ENDPOINT=https://api-cn-north-4.modelarts-maas.com
MAAS_MODEL_REASONER=deepseek-r1
MAAS_MODEL_WRITER=qwen-plus

# Huawei Cloud OCR Configuration
OCR_ENDPOINT=https://ocr.cn-north-4.myhuaweicloud.com
OCR_REGION=cn-north-4
OCR_AK=your_access_key_here
OCR_SK=your_secret_key_here
OCR_PROJECT_ID=your_project_id_here

# Application Settings
APP_ENV=development
APP_LOG_LEVEL=INFO
APP_MAX_UPLOAD_SIZE_MB=20
APP_ALLOWED_EXTENSIONS=.pdf,.png,.jpg,.jpeg,.tiff,.bmp

# Rate Limiting
MAX_REQUESTS_PER_MINUTE=60

# Security
ENABLE_REDACTION=true
