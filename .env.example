# Huawei Cloud ModelArts MaaS Configuration
MAAS_API_KEY=your_maas_api_key_here
MAAS_REGION=ap-southeast-1
MAAS_ENDPOINT=https://api-ap-southeast-1.modelarts-maas.com/v2/chat/completions
MAAS_MODEL_REASONER=deepseek-v3.1
MAAS_MODEL_WRITER=qwen3-32b

# Huawei Cloud OCR Configuration
OCR_ENDPOINT=https://ocr.ap-southeast-1.myhuaweicloud.com
OCR_REGION=ap-southeast-1
OCR_AK=your_ocr_access_key_here
OCR_SK=your_ocr_secret_key_here
OCR_PROJECT_ID=your_project_id_here

# Application Settings
APP_ENV=development
APP_LOG_LEVEL=INFO
APP_MAX_UPLOAD_SIZE_MB=20
APP_ALLOWED_EXTENSIONS=.pdf,.png,.jpg,.jpeg,.tiff,.bmp

# Rate Limiting
MAX_REQUESTS_PER_MINUTE=60

# Security
ENABLE_REDACTION=true
